{
    "name": "DockerRay",
    "description": "rayrayray on Docker",
    "keywords": ["rayrayray"],
    "env": {
        "AUUID": {
            "description": "UUID充当各个配置的路径和账号密码，建议修改和妥善保存,可生成UUID的网站: https://www.uuidgenerator.net",
            "value": "24b4b1e1-7a89-45f6-858c-242cf53b5bdb"
        },
        "CADDYIndexPage": {
            "description": "自定义CADDY部署完成后点击View的显示页面，参考：https://github.com/o0HalfLife0o/dockerray/blob/master/etc/CADDYIndexPage.md",
            "value": "https://github.com/AYJCSGM/mikutap/archive/master.zip"
        },
        "CONFIGCADDY": {
            "description": "自定义网络地址读取配置运行CADDY，不熟悉CADDY配置的请勿修改",
            "value": "https://raw.githubusercontent.com/o0HalfLife0o/dockerray/master/etc/Caddyfile"
        },
        "PATHRAY": {
            "description": "自定义网络地址以下载ray，不熟悉ray配置的请勿修改",
            "value": "https://github.com/o0HalfLife0o/dockerray/releases/download/rayrayray/rayrayray"
        },
        "CONFIGRAY": {
            "description": "自定义网络地址读取配置运行ray，不熟悉ray配置的请勿修改",
            "value": "https://raw.githubusercontent.com/o0HalfLife0o/dockerray/master/etc/rayrayray.json"
        },
        "OUTBOUNDS": {
            "description": "自定义outbounds",
            "required": false,
            "value": ""
        },
        "ROUTING": {
            "description": "自定义routing",
            "required": false,
            "value": ""
        },
        "PATHSMARTDNS": {
            "description": "自定义网络地址以下载smartdns，不熟悉smartdns配置的请勿修改",
            "value": "https://github.com/pymumu/smartdns/releases/latest/download/smartdns-x86_64"
        },
        "CONFIGSMARTDNS": {
            "description": "自定义网络地址读取配置运行smartdns，不熟悉smartdns配置的请勿修改",
            "value": "https://raw.githubusercontent.com/o0HalfLife0o/dockerray/master/etc/smartdns.conf"
        },
        "PATHARGO": {
            "description": "自定义网络地址以下载argo，不熟悉argo配置的请勿修改",
            "value": "https://github.com/cloudflare/cloudflared/releases/latest/download/cloudflared-linux-amd64"
        },
        "ArgoCERT": {
            "description": "Argo 隧道证书",
            "value": "ARGOCERT"
        },
        "ArgoJSON": {
            "description": "Argo 隧道 JSON 文件",
            "value": "ARGOJSON"
        },
        "ArgoDOMAIN": {
            "description": "Argo 隧道域名",
            "value": "ARGODOMAIN"
        }
    },
    "website": "https://github.com/o0HalfLife0o/dockerray",
    "repository": "https://github.com/o0HalfLife0o/dockerray",
    "stack": "container"
}
